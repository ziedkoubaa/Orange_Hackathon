    "@types/pdfjs-dist": "^2.10.377",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "~3.6.1",
    "eslint-plugin-import": "^2.31.0",
